@model AdminTableActionsViewModel

<script type="text/template" id="@Model.TemplateId">
    <div class="d-flex justify-content-end gap-2">
        <button type="button" class="btn btn-sm btn-secondary admin-edit" data-entity="@Model.EntityKey" data-id="{{id}}">
            <i class="bi bi-pencil"></i>
        </button>
        <button type="button" class="btn btn-sm btn-outline-danger admin-delete" data-entity="@Model.EntityKey" data-id="{{id}}">
            <i class="bi bi-trash"></i>
        </button>
    </div>
</script>
